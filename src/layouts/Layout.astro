  font-display: swap;
} */

      @font-face {
        font-family: "Gilroy-Light";
        src: url("/fonts/Gilroy-Light.woff");
        font-weight: normal;
        font-style: normal;
        font-display: swap;
      }

      @font-face {
        font-family: "Gilroy-Bold";
        src: url("/fonts/Gilroy-Bold.woff");
        font-weight: normal;
        font-style: normal;
        font-display: swap;
      }

      @font-face {
        font-family: "Gilroy-Medium";
        src: url("/fonts/Gilroy-Medium.woff");
        font-weight: normal;
        font-style: normal;
        font-display: swap;
      }

      @font-face {
        font-family: "Gilroy-Regular";
        src: url("/fonts/Gilroy-Regular.woff");
        font-weight: normal;
        font-style: normal;
        font-display: swap;
      }

      @font-face {
