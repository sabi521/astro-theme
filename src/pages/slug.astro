export async function getStaticPaths() {
  const response = await fetch('https://jsonplaceholder.typicode.com/posts')
  const { data } = await response.json()

  console.log(data);
  return data.map((post) => {
    return {
      params: { slug: post.slug },
      props: { post },
    };
  });
}

